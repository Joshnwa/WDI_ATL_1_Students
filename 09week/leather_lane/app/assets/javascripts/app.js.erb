angular.module('leatherLaneMarketApp', ['ngAnimate', 'ngRoute'])
.config(function($routeProvider) {
  $routeProvider
  .when('/market', {
    templateUrl: '<%= asset_path "market.html" %>',
    controller: 'MarketController'
  })
  .when('/add', {
    templateUrl: '<%= asset_path "add.html" %>',
    controller: 'AddController'
  })
  .otherwise({
    redirectTo: '/market'
  });
});
